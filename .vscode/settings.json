{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#550060",
    "activityBar.background": "#550060",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#807100",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#550060",
    "statusBar.background": "#28002d",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#550060",
    "statusBarItem.remoteBackground": "#28002d",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#28002d",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#28002d99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#28002d",
  "deno.enablePaths": ["./supabase"]
}
